  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
  "private": true,
  "description": "ConnectMe provides a quick and confidential method for students to book appointments with their school's service workers.",
  "scripts": {
    "postinstall": "cd server && npm install && cd .. && cd react-ui && npm install",
    "start:dev": "start npm run --prefix server start && start npm run --prefix react-ui start",
    "heroku-postbuild": "npm run --prefix react-ui build",
    "start": "cd server && NODE_ENV=production node server.js"
  },
